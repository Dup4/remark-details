{
    "compilerOptions": {
        "target": "es2020",
        "module": "es2020",
        "sourceMap": true,
        "declaration": true,
        "strict": true,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "../test_dist",
    },
    "include": [
        "./*.ts"
    ]
}
